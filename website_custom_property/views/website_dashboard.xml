<odoo>
   <template id="my_properties_content">      
               <div class="container">
               <div class="card">
               <div class="card-header">
               <div class="row">
                  <div class="col-xl-2 col-lg-2 col-md-12 col-sm-12"></div>
                  <div class="col-xl-8 col-lg-8 col-md-12 col-sm-12">
                    Bienvenido <strong><t t-esc="request.env['res.users'].search([('id','=',request.session.uid)]).name"/></strong>
                    <input type="hidden" id="loggin" t-att-value="request.session.uid"/>
                  </div>
                  <div class="col-xl-2 col-lg-2 col-md-12 col-sm-12"> 
                      <a class="btn btn-primary pull-left" href="/clock_alert"><i class="fas fa-clock"></i><span id="clock_alert_count"></span></a>    
                     <a class="btn btn-primary pull-left" id="calendary_show" href="/propiedades/calendario"><i class="fas fa-calendar"></i></a>            
                     
                  </div>
               </div>
               </div>
               <div class="card-body">
               <div class="row">
               <div class="col-xl-5 col-lg-5 col-md-12 col-sm-12 border">    
                  <h5> <strong>Totales de rentas:</strong> <span t-esc="suma_per_amount"/>  
                  <strong>Programado:</strong> <span t-esc="suma_espera"/> </h5>    
                  <div class="table-responsive border rounded border-top-0 ">
                   <table class="table rounded mb-0 bg-white o_portal_my_doc_table">
                            <thead>
                                <tr>
                                    <th>Estado</th>
                                    <th>Nombre</th>
                                    <th>Rentas</th>
                                    <th>Programado</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr t-foreach="todos_propidades" t-as="vals">
                                   <td><span t-esc="vals['estado']"/></td>
                                   <td>
                                    <form action="/property_details">
                                      <input type="hidden" id="propiedad_evaluar" t-att-value="vals['evaluar']" name="propiedad_evaluar"/>
                                      <button type="submit" class="btn btn-link" ><span t-esc="vals['propiedad']"/></button>
                                   </form>
                                   </td>
                                   <td><span t-esc="vals['suma_debe']"/></td>  
                                   <td><span t-esc="vals['suma_haber']"/></td>                                 
                                </tr>
                            </tbody>
                       </table>
                    </div>
                    
              </div>   
               <div class="col-xl-5 col-lg-5 col-md-12 col-sm-12 border">
                <center><h5>Contratos por vencer</h5></center>   
                <u1 class="list-group">
                  <li class="list-group-item d-flex justify-content-between align-items-center">
                  <a id="contratos_uno" href="/rango_menos_15"> Vence en 15 días o menos:</a>
                   Inquilino<span class="badge badge-primary badge-pill">
                  <t t-esc="contratos_inquilinos[0]"/></span>
                   Propietario<span class="badge badge-primary badge-pill">
                  <t t-esc="contratos_propietario[0]"/></span>
                  </li>
                   <li class="list-group-item d-flex justify-content-between align-items-center">
                   <a id="contratos_dos" href="/rango_16_30">Vence en 16-30 días:</a>
                  Inquilino<span class="badge badge-primary badge-pill">
                  <t t-esc="contratos_inquilinos[1]"/></span>
                  Propietario<span class="badge badge-primary badge-pill">
                  <t t-esc="contratos_propietario[1]"/></span>
                  </li>
                    <li class="list-group-item d-flex justify-content-between align-items-center">
                   <a id="contratos_tres" href="/rango_31_60">Vence en 31-60 días:</a>
                    Inquilino<span class="badge badge-primary badge-pill">
                  <t t-esc="contratos_inquilinos[2]"/></span>
                  Propietario<span class="badge badge-primary badge-pill">
                  <t t-esc="contratos_propietario[2]"/></span>
                  </li>
                  <li class="list-group-item d-flex justify-content-between align-items-center">
                   <a  id="contratos_tres" href="/rango_mas_60">Vence en más de 60 días:</a>
                  Inquilino<span class="badge badge-primary badge-pill">
                  <t t-esc="contratos_inquilinos[3]"/></span>
                  Propietario<span class="badge badge-primary badge-pill">
                  <t t-esc="contratos_propietario[3]"/></span>
                  </li>
                </u1>                              
               </div>           
               
                <div class="col-xl-2 col-lg-2 col-md-12 col-sm-12 border">
                   <h5><strong>Estado de renta</strong></h5>
                   <input type="text" class="form-control" name="dates"/>
                    <label for="cobro_gestor">Cobradas por el gestor</label>
                    <input type="text" class="form-control" name="cobro_gestor"  id="cobro_gestor"/>
                    <label for="admin">Administración</label>
                    <input type="text" class="form-control" name="admin" id="admin"/>
                    <label for="mantenimiento">Mantenimientos</label>
                    <input type="text" class="form-control" name="mantenimiento" id="mantenimiento"/>
                    <label for="otros_gastos">Otros gastos</label>
                    <input type="text" class="form-control" name="otros_gastos" id="otros_gastos"/>
                  
                </div>      
               </div>  
              
               <div class="row">               
                  <center><h2>Mantenimientos</h2></center> 
                  <input type="hidden" name="titles" id="titles"/>
                   <div id="kanban"></div>
                  </div>                      
               </div>
               </div>
               </div>
               
           
   </template>
</odoo>