<odoo>
   <template id="maintenance_details" name="My maintenance details">
    <t t-call="portal.frontend_layout">
        <div class="container">
           <h1>Detalles del Mantenimiento</h1>
           <t t-esc="manteni.name"/>
           <div class="row">
             <div class="col-xl-6 col-lg-6 col-md-12 col-sm-12">
                <strong>Propiedad</strong> <t t-esc="manteni.property_id.name"/><br/>
                <strong>Fecha de Solicitud</strong> <t t-esc="manteni.request_date"/><br/>
                <strong>Fecha de Cierre</strong> <t t-esc="manteni.close_date"/><br/>
                <strong>Tipo de Matenimiento</strong> <t t-esc="manteni.maintenance_type"/><br/>
                <strong>Inquilino</strong> <t t-esc="manteni.tenant_id.name"/><br/>
             </div>
             <div class="col-xl-6 col-lg-6 col-md-12 col-sm-12">
               <strong>Equipo</strong> <t t-esc="manteni.maintenance_team_id.name"/><br/>
               <strong>Resposable</strong> <t t-esc="manteni.user_id.name"/><br/>
               <strong>Fecha Prevista</strong> <t t-esc="manteni.schedule_date"/><br/>
               <strong>Duracion</strong> <t t-esc="manteni.duration"/><br/>
               <strong>Costo</strong> <t t-esc="manteni.cost"/><br/>
               <strong>Culpa de los inquilinos</strong> <t t-esc="manteni.renters_fault"/><br/>
               <strong>Factura</strong> <t t-esc="manteni.invc_id.name"/><br/>
               <strong>Fecha de Factura</strong> <t t-esc="manteni.date_invoice"/><br/>
               <strong>Prioridad</strong> <t t-esc="manteni.priority"/><br/>
               <strong>Compa√±ia</strong> <t t-esc="manteni.company_id.name"/><br/>
             </div>
           </div>
           
        </div>
    </t>
   </template>
</odoo>