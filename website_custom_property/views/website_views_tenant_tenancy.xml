<odoo>
   <template id="my_properties_tenancy" name="My Tenancy">
           <t t-call="portal.frontend_layout">
            <div class="container border">                
             <center><t t-esc="titulo_inq"/></center>
             <div class="table-responsive border rounded border-top-0 ">
              <table class="table rounded mb-0 bg-white o_portal_my_doc_table">
                    <thead>
                      <tr>
                      <th>#</th>
                      <th>Nombre</th>
                      <th>Propiedad</th>
                      <th>Inquilino</th>
                      <th>Fecha inicio</th>
                      <th>Fecha fin</th>
                      <th>Tipo renta</th>
                      <th>Renta</th>
                      <th>Estado</th>
                      </tr>
                    </thead>
                    <tbody>
                       <tr t-foreach="lista_contatos_inq" t-as="o">
                         <td>
                          <form action="/tenant_details">                            
                            <input  type="hidden" id="contrato" t-att-value="o.id" name="contrato"/>
                           <button type="submit" class="btn btn-link" ><t t-esc="o.code"/></button>
                           </form>
                         </td>
                         <td><t t-esc="o.name"/></td>
                         <td><t t-esc="o.property_id.name"/></td>
                         <td><t t-esc="o.tenant_id.name"/></td>
                         <td><t t-esc="o.date_start"/></td>
                         <td><t t-esc="o.date"/></td>
                         <td><t t-esc="o.rent_type_id.name"/></td>
                         <td><t t-esc="o.rent"/></td>
                         <td>
                         <t t-if="o.state == 'template'">
                          Template
                         </t>
                         <t t-if="o.state == 'draft'">
                          Nuevo
                         </t>
                         <t t-if="o.state == 'open'">
                          Abierto
                         </t>
                         <t t-if="o.state == 'pending'">
                          Renovacion
                         </t>
                         <t t-if="o.state == 'close'">
                          Close
                         </t>
                         <t t-if="o.state == 'cancelled'">
                          Cancelado
                         </t>                           
                         </td>
                       </tr>
                    </tbody>
                 </table>
             </div>
             <center><t t-esc="titulo_pro"/></center>
              <div class="table-responsive border rounded border-top-0 ">
              <table class="table rounded mb-0 bg-white o_portal_my_doc_table">
                  <thead>
                      <tr>
                      <th>#</th>
                      <th>Nombre</th>
                      <th>Propiedad</th>
                      <th>Inquilino</th>
                      <th>Fecha inicio</th>
                      <th>Fecha fin</th>
                      <th>Tipo renta</th>
                      <th>Renta</th>
                      <th>Estado</th>
                      </tr>
                    </thead>
                     <tbody>
                       <tr t-foreach="lista_contatos_pro" t-as="o">
                         <td>
                          <form action="/tenant_details">                            
                            <input  type="hidden" id="contrato" t-att-value="o.id" name="contrato"/>
                           <button type="submit" class="btn btn-link" ><t t-esc="o.code"/></button>
                           </form>
                         </td>
                         <td><t t-esc="o.name"/></td>
                         <td><t t-esc="o.property_id.name"/></td>
                         <td><t t-esc="o.tenant_id.name"/></td>
                         <td><t t-esc="o.date_start"/></td>
                         <td><t t-esc="o.date"/></td>
                         <td><t t-esc="o.rent_type_id.name"/></td>
                         <td><t t-esc="o.rent"/></td>
                         <td><t t-esc="o.state"/></td>
                       </tr>
                    </tbody>
              </table>
              </div>
            </div>
             
           </t>
   </template>
</odoo>